{
  "name": "fooling-around",
  "version": "1.0.0",
  "main": "index.ts",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "android:emulator": "expo run:android --device Better_Phone_API_35",
    "prebuild": "expo prebuild --clean",
    "prebuild:android": "expo prebuild --clean --platform android",
    "lint": "expo lint",
    "lint:fix": "expo lint --fix",
    "preinstall": "npx only-allow yarn",
    "ios": "expo run:ios",
    "knip": "knip",
    "madge": "npx madge",
    "madge:circular": "npx madge --circular"
  },
  "dependencies": {
    "@nozbe/watermelondb": "^0.28.0",
    "@sentry/react-native": "^7.6.0",
    "@turf/turf": "^7.2.0",
    "expo": "~54.0.23",
    "expo-dev-client": "~6.0.17",
    "expo-location": "~19.0.7",
    "expo-status-bar": "~3.0.8",
    "expo-task-manager": "~14.0.8",
    "react": "19.1.0",
    "react-native": "0.81.5"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@types/node": "^24.10.1",
    "@types/react": "~19.1.0",
    "eslint": "^9.0.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "knip": "^5.70.1",
    "madge": "^8.0.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3"
  },
  "madge": {
    "tsConfig": "./tsconfig.json",
    "fileExtensions": [
      "ts",
      "tsx"
    ],
    "detectiveOptions": {
      "ts": {
        "skipTypeImports": true
      }
    }
  }
